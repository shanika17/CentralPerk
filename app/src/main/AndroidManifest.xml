<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.log">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Central Perk"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Log">
        <activity
            android:name=".RoomUpdateActivity"
            android:parentActivityName=".RoomListActivity"></activity>
        <activity
            android:name=".RoomAddActivity"
            android:label="Book a Room"
            android:parentActivityName=".RoomListActivity" />
        <activity
            android:name=".RoomListActivity"
            android:label="My Bookings"
            android:parentActivityName=".RoomActivity" />
        <activity
            android:name=".OfferActivity"
            android:label="Special Offers"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".SpaActivity"
            android:label="Spa Treatments"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".FoodActivity"
            android:label="Food"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".EventActivity"
            android:label="Events"
            android:parentActivityName=".Welcome" />
        <activity
            android:name=".RoomActivity"
            android:label="Rooms"
            android:parentActivityName=".Welcome" />
        <activity android:name=".Welcome" />
        <activity android:name=".LoginForm" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>